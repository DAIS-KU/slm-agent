perigee_distance_km = 363300  # km (minimum distance between Earth and Moon)    
                                                                                
# Eliud Kipchoge's marathon time: 2 hours 30.583 minutes                        
# Convert to hours                                                              
marathon_time_hours = 2 + (30.583 / 60)  # 30.583 minutes in hours              
                                                                                
# Marathon distance: 26.2 miles                                                 
# Convert miles to km: 1 mile = 1.60934 km                                      
marathon_distance_km = 26.2 * 1.60934                                           
                                                                                
# Calculate pace in km/h                                                        
pace_kmh = marathon_distance_km / marathon_time_hours                           
                                                                                
# Total time to run perigee distance                                            
total_time_hours = perigee_distance_km / pace_kmh                               
                                                                                
# Convert to thousand hours and round to nearest 1000                           
thousand_hours = round(total_time_hours / 1000) * 1000                          
                                                                                
final_answer(thousand_hours)                                                    
